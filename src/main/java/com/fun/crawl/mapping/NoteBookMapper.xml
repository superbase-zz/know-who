<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fun.crawl.mapper.NoteBookMapper">

<!-- 通用查询映射结果 -->
<resultMap id="BaseResultMap" type="com.fun.crawl.model.NoteBook">
    <id column="id" property="id"/>
        <result column="note_user_id" property="noteUserId"/>
      <result column="notebook_guid" property="notebookGuid"/>
      <result column="notebook_name" property="notebookName"/>
      <result column="notebook_type" property="notebookType"/>
      <result column="is_deleted" property="isDeleted"/>
      <result column="create_time" property="createTime"/>
      <result column="modify_time" property="modifyTime"/>
  </resultMap>

<!-- 通用查询结果列 -->
<sql id="Base_Column_List">
       id, note_user_id, notebook_guid, notebook_name, notebook_type, is_deleted, create_time, modify_time
</sql>
<select id="pageByQuery" resultMap="BaseResultMap">
    select     id, note_user_id, notebook_guid, notebook_name, notebook_type, is_deleted, create_time, modify_time
    from t_note_book
    <where>
        <if test="id !=null and id!=''">
           and id = #{ id}
        </if>
        <if test="noteUserId !=null and noteUserId!=''">
           and note_user_id = #{ noteUserId}
        </if>
        <if test="notebookGuid !=null and notebookGuid!=''">
           and notebook_guid = #{ notebookGuid}
        </if>
        <if test="notebookName !=null and notebookName!=''">
           and notebook_name = #{ notebookName}
        </if>
        <if test="notebookType !=null and notebookType!=''">
           and notebook_type = #{ notebookType}
        </if>
        <if test="isDeleted !=null and isDeleted!=''">
           and is_deleted = #{ isDeleted}
        </if>
        <if test="createTime !=null and createTime!=''">
           and create_time = #{ createTime}
        </if>
        <if test="modifyTime !=null and modifyTime!=''">
           and modify_time = #{ modifyTime}
        </if>
    </where>
</select>


</mapper>
